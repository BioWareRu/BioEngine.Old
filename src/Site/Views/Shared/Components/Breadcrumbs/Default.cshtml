@model List<BioEngine.Site.ViewModels.BreadCrumbsItem>
<nav class="breadcrumbs__block">
    <ul class="breadcrumbs">
        @foreach (var breadcrumb in Model)
        {
            <li class="breadcrumbs__item">
                <a class="breadcrumbs__link" href="@breadcrumb.Url">@Html.Raw(breadcrumb.Title)</a>
            </li>
        }
    </ul>
</nav>